<div class="well">
  <div class="well"><h1>Listing bookings</h1></div>
  <div class="well">
  <%= link_to "Book a room", new_booking_path, remote: true, id:'list_bookings', 
  class:'btn btn-info btn-lg' %>
  </div>
  <div class="well">
    <table class="table table-bordered">
        <tr>
          <th>Check in</th>
          <th>Check out</th>
          <th>Adults</th>
          <th>Childrens</th>
          <th>Room number</th>
          <th>Customer name</th>
          <th colspan="3">Actions</th>
        </tr>
    </table>
  </div>
</div>